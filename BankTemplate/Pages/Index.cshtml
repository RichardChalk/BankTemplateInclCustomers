@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<h1>Alla Customers</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var c in Model.Customers)
        {
            <tr>
                <td>@c.Id</td>
                <td>@c.Name</td>
            </tr>
        }
    </tbody>
</table>